{
  "name": "gab-alarm",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --names SERVER,CLIENT --prefix-colors 'yellow,blue' --kill-others 'pnpm run --filter ./packages/server dev' 'pnpm run --filter ./packages/client dev'",
    "start": "pnpm run --filter ./packages/server start",
    "build": "pnpm run --filter ./packages/shared build && pnpm run --filter ./packages/client build",
    "lint": "xo",
    "preview": "vite preview"
  },
  "dependencies": {},
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.10.8",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.0.2",
    "typescript": "^5.2.2",
    "xo": "^0.56.0"
  }
}